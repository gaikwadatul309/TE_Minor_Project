function popup_confirm_submit()
{
    $(function () 
	{
        $("#dialog").dialog(
		{
			width: 500,
			modal: true,
			resizable: false,
			buttons: 
			{
				"Confirm": function () 
				{
					$(this).dialog("close");
					document.getElementById("dialog").innerHTML = "";
					$("#submit").click();
				},
				"Edit": function () 
				{
					$(this).dialog("close");
					document.getElementById("dialog").innerHTML = "";
				}
			}
		});
    });
}
