	var iti_displain=document.getElementById("iti_discipline").value;
	var dip_displain=document.getElementById("diploma_discipline").value;
	var degree_displain=document.getElementById("degree_discipline").value;
	
	document.getElementById('vh_b_status').value=0;
	document.getElementById('vh_b').checked=false;
	
	document.getElementById('vh_lv_status').value=0;
