function popup_alert5(val3,field,field_row,field_ncl)
{
    $(function () 
	{
        $("#dialog12").dialog(
		{
			width: 500,
			modal: true,
			resizable: false,
			buttons: 
			{
				"Confirm": function () 
				{
					$(this).dialog("close");
					document.getElementById("dialog12").innerHTML = "";
					
					if (val3=='SC' || val3=='ST' || val3=='OBC-NON CREAMY LAYER')
					{
						$("#res_obc_ncl_note").hide();
						$(".reservation_row").show();
						$("#ebc_display_row").show();
					}
					else if (val3=='OBC-CREAMY LAYER')
					{
						$(".reservation_row").hide();
						$("#res_obc_ncl_note").show();
						$("#ebc_display_row").hide();
						controlebc_display('No');
						document.submit_form.ebc[0].checked=false;
						document.submit_form.ebc[1].checked=false;
					}
				},
				"No": function () 
				{
					$(this).dialog("close");
					document.getElementById(field).value = "";
					$("."+field_row).hide();
					$("#"+field_ncl).hide();
					$("#ebc_display_row").show();
					document.getElementById("dialog12").innerHTML = "";
				}
			}
		});
    });
}
