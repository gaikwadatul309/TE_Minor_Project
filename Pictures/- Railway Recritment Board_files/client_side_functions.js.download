function popup_alert(value)
{
    $(function ()
    {
        $("#dialog")
            .dialog(
            {
                width: 500,
                modal: true,
                resizable: false,
                buttons:
                {
                    "Ok": function ()
                    {
                        $(this).dialog("close");
                        document.getElementById(value).focus();
                        document.getElementById("dialog").innerHTML="";
                    }
                }
            });
    });
}
