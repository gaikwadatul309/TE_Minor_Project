function popup_confirm_tenth(val3,field)
{
    $(function () 
	{
        $("#dialog").dialog(
		{
			width: 500,
			modal: true,
			resizable: false,
			buttons: 
			{
				"Confirm": function () 
				{
					$(this).dialog("close");
					document.getElementById("dialog").innerHTML = "";
				},
				"No": function () 
				{
					$(this).dialog("close");
					document.getElementById(field).value = "";
					document.getElementById("dialog").innerHTML = "";
				}
			}
		});
    });
}
