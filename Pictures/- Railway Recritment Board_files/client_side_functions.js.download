function emailcheck(str) 
{
    var at="@"
    var dot="."
    var lat=str.indexOf(at)
    var lstr=str.length
    var ldot=str.indexOf(dot)
    if (str.indexOf(at)==-1) 
	{  
		//alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("You must enter your full name");
		popup_alert("email");
		return false;
    }
    if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr) 
	{
       	//alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");
		return false;
    }
    if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr) 
	{
       	//alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");

		return false;
    }
    if (str.indexOf(at, (lat + 1))!=-1) 
	{
       	//alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");
		return false;
    }
    if (str.substring(lat - 1, lat)==dot || str.substring(lat + 1, lat + 2)==dot) 
	{
        //alert  ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");
		return false;
    }
    if (str.indexOf(dot, (lat + 2))==-1)
	{
       	//alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");
		return false;
    }
    if (str.indexOf(" ")!=-1)
	{
        //alert ("The eMail address '@' convention appears to be invalid");
		document.getElementById("dialog").innerHTML=("The eMail address '@' convention appears to be invalid");
		popup_alert("email");
		return false;
    }
    return true
}

function isDate(dateStr)
{
    var datePat=/^(\d{1,2})(\/|-)(\d{1,2})(\/|-)(\d{4})$/;
    // alert(dateStr);
    var matchArray=dateStr.match(datePat); // is the format ok?
    // alert(dateStr);

    month=matchArray[3]; // p@rse date into variables
    day=matchArray[1];
    year=matchArray[5];
    if (month < 1 || month > 12)
    { // check month range
        document.getElementById("dialog").innerHTML=("Month must be between 1 and 12.");
        popup_alert("dob_m");
        return false;
    }
    if (day < 1 || day > 31)
    {
        document.getElementById("dialog").innerHTML=("Day must be between 1 and 31.");
        popup_alert("dob_d");
        return false;
    }
    if ((month==4 || month==6 || month==9 || month==11) && day==31)
    {
        document.getElementById("dialog").innerHTML=("Month " + month + " doesn`t have 31 days!");
        popup_alert("dob_m");
        return false;
    }
    if (month==2)
    { // check for february 29th
        var isleap=(year % 4==0 && (year % 100!=0 || year % 400==0));
        if (day > 29 || (day==29 && !isleap))
        {
            document.getElementById("dialog").innerHTML=("February " + year + " doesn`t have " + day + " days!");
            popup_alert("dob_m");
            return false;
        }
    }
    return true; // date is valid
}


