		if(val=='hh' && document.getElementById("hh_d").checked==true)
		{
			document.getElementById("dialog").innerHTML = ("You must select either Hearing Impairment - Deaf (D) (or) Hearing Impairment - Hard of hearing (HH)");
			popup_alert("hh_d");
			document.getElementById("hh_d").checked=false;
		}
		if(val=='hh_d' && document.getElementById("hh").checked==true)
		{
			document.getElementById("dialog").innerHTML = ("You must select either Hearing Impairment - Deaf (D) (or) Hearing Impairment - Hard of hearing (HH)");
			popup_alert("hh");
			document.getElementById("hh").checked=false;
		}
		
		if(vh_b_check==1 || vh_lv_check==1 || oh_oa_check==1 || oh_ol_check==1 || oh_bl_check==1 || hh_check==1 || hh_d_check==1 || lc_check==1 || aav_check==1 || dorf_check==1)
		{
			var tenth_qualifi=document.getElementById("tenth_qualification").value;
			var tenth_displain=document.getElementById("tenth_discipline").value;
			
			var iti_qualifi=document.getElementById("iti_qualification").value;
