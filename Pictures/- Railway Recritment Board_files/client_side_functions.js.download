function checkphoto()
{
	var id1 = document.getElementById("photo").value;
	var dot = id1.lastIndexOf('.')
	var ext = id1.substring(dot+1);
	ext=ext.toUpperCase();
	
	if(ext=='') 
	{
		 document.getElementById("dialog").innerHTML=("Please select your Photo with Signature");
		 popup_alert("photo");
		 return false;
	} 
	else if(ext != "JPG" && ext != "JPEG")
	{
		 document.getElementById("dialog").innerHTML=("Use JPG / JPEG formats only for Photo with Signature");
		 popup_alert("photo");
		 return false;
	}
		
	var fi = document.getElementById("photo");
	if(((fi.files[0].size)/1000)>30) 
	{
		 document.getElementById("dialog").innerHTML=("Photo with Signature image size must be lesser than 30kb");
		 popup_alert("photo");
		 return false;
	}	
	if(((fi.files[0].size)/1000)<10) 
	{
		 document.getElementById("dialog").innerHTML=("Photo with Signature image size must be greater than 10kb");
		 popup_alert("photo");
		 return false;
	}		
}
