			xajax_check_pwd_available_status(tenth_qualifi,tenth_displain,iti_qualifi,iti_displain,dip_qualifi,dip_displain,degree_qualifi,degree_displain,vh_b_check,vh_lv_check,oh_oa_check,oh_ol_check,oh_bl_check,hh_check,hh_d_check,lc_check,aav_check,dorf_check,val);
		}
		
		
	}
