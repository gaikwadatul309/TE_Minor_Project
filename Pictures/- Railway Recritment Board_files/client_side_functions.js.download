	document.getElementById('oh_bl').checked=false;
	
	document.getElementById('hh_status').value=0;
	document.getElementById('hh').checked=false;
	
	document.getElementById('hh_d_status').value=0;
	document.getElementById('hh_d').checked=false;
	
	document.getElementById('lc_status').value=0;
	document.getElementById('lc').checked=false;
	
	document.getElementById('aav_status').value=0;
	document.getElementById('aav').checked=false;
	
	document.getElementById('dorf_status').value=0;
	document.getElementById('dorf').checked=false;
	
	var rrb_id=document.getElementById("rrb_id_status").value;
	
	if((tenth_qualifi!="" && tenth_displain!="") || (iti_qualifi!="" && iti_displain!="") || (dip_qualifi!="" && dip_displain!="") || (degree_qualifi!="" && degree_displain!=""))
	{
    	xajax_discipline_pwd_display(tenth_qualifi,tenth_displain,iti_qualifi,iti_displain, dip_qualifi,dip_displain,degree_qualifi,degree_displain);
