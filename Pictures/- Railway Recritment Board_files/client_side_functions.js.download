function checkrelax(maxagecat, dob_date, res_val)
{
    var relaxation=0;
    var assign=0;
	var unlimited=0;
	
	if (document.submit_form.pwd[0].checked==true)
	{
		assign=10;
		if (assign > relaxation)
		{
			relaxation=assign;
		}
	}
	
	if (document.submit_form.ex_military[0].checked==true)
	{
		var exmp_joining=document.getElementById("exmp_joining_d").value+"-"+document.getElementById("exmp_joining_m").value+"-"+document.getElementById("exmp_joining_y").value;
		var exmp_discharge=document.getElementById("exmp_discharge_d").value+"-"+document.getElementById("exmp_discharge_m").value+"-"+document.getElementById("exmp_discharge_y").value;
		
		var out=checkAge(exmp_joining,exmp_discharge);
		var las=out.split('-');
		if(parseInt(las[0])>=1 || parseInt(las[1])>=6)
		{
			assign=3 +  parseInt(las[0]);
			if (assign > relaxation)
			{
				relaxation=assign;
			}
		}
	}
	if (document.submit_form.apprenticeship_course[0].checked==true)
	{
		var apprenticeship_from=document.getElementById("apprenticeship_from_d").value+"-"+document.getElementById("apprenticeship_from_m").value+"-"+document.getElementById("apprenticeship_from_y").value;
		var apprenticeship_to=document.getElementById("apprenticeship_to_d").value+"-"+document.getElementById("apprenticeship_to_m").value+"-"+document.getElementById("apprenticeship_to_y").value;
		
		var out=checkAge(apprenticeship_from,apprenticeship_to);
		var las=out.split('-');
		/*var dob_can=document.getElementById("dob_age").value;
		var dob_can=dob_can.split('-');
		var dob_can1=dob_can[2]+'-'+dob_can[1]+'-'+dob_can[0];*/
		
		if(document.getElementById("application_edit_status").value==1)
		{
			var dob_can1= document.getElementById("dob_d").value+'-'+document.getElementById("dob_m").value+'-'+document.getElementById("dob_y").value;
		}
		else
		{
			var dob_can=document.getElementById("dob_age").value;
			var dob_can=dob_can.split('-');
			var dob_can1=dob_can[2]+'-'+dob_can[1]+'-'+dob_can[0];
		}
		
		
		var out=checkAge(dob_can1,apprenticeship_from);
		var las=out.split('-');
	
		if(las[0]<=25)
		{
			assign= 5;
			if (assign > relaxation)
			{
				relaxation=assign;
			}
		}
		else
		{
			var out=checkAge(apprenticeship_from,apprenticeship_to);
			var las=out.split('-');
			
			assign= parseInt(las[0]);
			if (assign > relaxation)
			{
				relaxation=assign;
			}
		}
	}
	
	if (document.submit_form.jammu_kashmir[0].checked==true)
	{
		assign=5;
		if (assign > relaxation)
		{
			relaxation=assign;
		}
	}
	if (document.submit_form.rly_emp[0].checked==true && document.submit_form.rly_service[0].checked==true)
	{
		
		assign=10;
		if (assign > relaxation)
		{
			relaxation=assign;
		}
	}
	if (document.submit_form.quasi_admin[0].checked==true)
	{
		var quasi_from=document.getElementById("quasi_from_d").value+"-"+document.getElementById("quasi_from_m").value+"-"+document.getElementById("quasi_from_y").value;
		var quasi_to=document.getElementById("quasi_to_d").value+"-"+document.getElementById("quasi_to_m").value+"-"+document.getElementById("quasi_to_y").value;
		
		var out=checkAge(quasi_from,quasi_to);
		var las=out.split('-');
		
		if(parseInt(las[0])>5)
		{
			assign=5;
		}
		else
		{
			assign=parseInt(las[0])
		}
		
		if (assign > relaxation)
		{
			relaxation=assign;
		}
	}
	if (document.submit_form.widow_divorce[0].checked==true)
	{
		assign=5;
		if (assign > relaxation)
		{
			relaxation=assign;
		}
	}
	
	
   	if (relaxation > 0)
    {
        maxagecat.setFullYear(maxagecat.getFullYear() - relaxation);
    }
	if(unlimited==0)
	{
		if (dob_date < maxagecat)
		{
			document.getElementById("dialog").innerHTML=("You are not eligible for this post, you are over aged");
			popup_alert("reservation");
			return false;
		}
	}
}
