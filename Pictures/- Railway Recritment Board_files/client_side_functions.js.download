	else if (((document.getElementById(field + "_d").value>01) && (str==7)) || str>7)
    {
        for (i=max_value - 1; i >=1950; i--)
        {
            select5.options[select5.options.length]=new Option(i, i);
        }
    }
	else if (((document.getElementById(field + "_d").value>02) && (str==12)))
    {
       if (field=='date_discharge' || field=='dob')
	   {
